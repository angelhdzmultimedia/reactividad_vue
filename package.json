{
  "type": "module",
  "name": "node-starter",
  "version": "0.0.0",
  "scripts": {
    "start:servidor": "nodemon servidor/src/main.js",
    "start:cliente": "cd cliente && npm install && npm run dev",
    "connect:cliente": "node start_cliente.js",
    "dev": "npm install &&  concurrently \"npm run start:servidor\" \"npm run connect:cliente\""
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "node-fetch": "^3.2.4",
    "nodemon": "^2.0.16",
    "sequelize": "^6.20.1",
    "sqlite3": "^5.0.8"
  }
}
